   </table>
  </div>
  <div id='data'>
  <table id="data_table">  
    <thead>
      <tr><td>WEEK</td><td class='table_heading_td'>Mon</td><td class='table_heading_td'>Tue<td class='table_heading_td'>Wed</td><td class='table_heading_td'>Thu</td><td class='table_heading_td'>Fri</td><td class='table_heading_td'>Sat</td><td class='table_heading_td'>Sun</td><td class='table_heading_td'>Total</td></tr>
    </thead>
    <%= Report.detail_user_encounter_results_html(@results,@stats_name,@user.name) %>
  </table>
  </div>
   <div class = "buttonsDiv">
     <button id='done_button' onmousedown='closePage();'>Done</button>
   </div>
